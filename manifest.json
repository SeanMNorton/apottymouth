{
  "manifest_version": 2,
  "name": "A Potty Mouth",
  "version": "0.2",
  "description": "Adds colorful language to the web.",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.0.min.js","content.js"]
    }
  ],
"icons": {"16":"icon.png", "48":"icon.png", "128":"icon.png"}
}
